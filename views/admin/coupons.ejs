<script>
    function unblock(id) {
        Swal.fire({
            title: "Are you sure?",
            text: "You want to unblock this coupon",
            showCancelButton: true,
            confirmButtonColor: "#000000",
            cancelButtonColor: "#d33",
            confirmButtonText: "Confirm"
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: "/admin/couponUnblock",
                    method: "post",
                    data: {
                        id
                    },
                    success: (response) => {
                        if (response.status === "okay") {
                            Swal.fire({
                                icon: "success",
                                title: "Coupon Unblocked",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                            setTimeout(() => {
                                window.location.reload()
                            }, 1000);
                        } else if (response.status === "oops") {
                            Swal.fire({
                                icon: "error",
                                title: "Somthing wrong",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                        }
                    }
			    })
            }
        });
    }

    function block(id) {
        Swal.fire({
            title: "Are you sure?",
            text: "You want to block this user",
            showCancelButton: true,
            confirmButtonColor: "#000000",
            cancelButtonColor: "#d33",
            confirmButtonText: "Confirm"
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: "/admin/couponBlock",
                    method: "post",
                    data: {
                        id
                    },
                    success: (response) => {
                        if (response.status === "okay") {
                            Swal.fire({
                                icon: "success",
                                title: "Coupon blocked",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                            setTimeout(() => {
                                window.location.reload()
                            }, 1000);
                        } else if (response.status === "oops") {
                            Swal.fire({
                                icon: "error",
                                title: "Somthing wrong",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                        }
                    }
			    })
            }
        });
    }

    function deletecoupon(id){
        Swal.fire({
            title: "Are you sure?",
            text: "You want to delete this coupon?",
            showCancelButton: true,
            confirmButtonColor: "#FF0000",
            cancelButtonColor: "#000000",
            confirmButtonText: "Delete",
            customClass: {
                popup: 'swal-custom',
                cancelButton: 'swal-custom-ok-btn'
            }
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: "/admin/addCoupon",
                    method: "delete",
                    data: {
                        id
                    },
                    success: (response) => {
                        if (response.status === "okay") {
                            Swal.fire({
                                icon: "success",
                                title: "Coupon removed",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                            setTimeout(() => {
                                window.location.reload()
                            }, 1000);
                        } else if (response.status === "oops") {
                            Swal.fire({
                                icon: "error",
                                title: "Somthing wrong",
                                showConfirmButton: false,
                                customClass: {
                                    popup: 'swal-custom',
                                },
                                timer: 1500
                            });
                        }
                    }
			})
            }
        });
    }
</script>

