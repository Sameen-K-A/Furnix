        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title fw-bold" style="margin: auto;">Orders</h2>
            </div>
            
            <div style="width: 100%; height: 50px; margin-bottom: 20px;" class="d-flex">
            <button onclick="orderdata()" class="orderbtn" id="orderbtn">Order details</button>
            <button onclick="returndata()" class="returnbtn" id="returnbtn">Return details</button>
            </div>
            <div class="card mb-4" id="orderdetails" style="display: block;">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead class="text-center " style="background-color: rgb(223, 223, 223);">
                                <tr>
                                    <th>OrderID</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Time</th>
                                    <th scope="col"> Action </th>
                                </tr>
                            </thead>
                            <tbody class="text-center">
                                <% for( let i=0; i < orderData.length; i++ ) { %>
                                    <% if (orderData[i].status==="Ordered" || orderData[i].status==="Cancelled" || orderData[i].status==="Shipped" || orderData[i].status==="Delivered") { %>
                                        <tr>
                                            <td>
                                                <%= orderData[i].orderID %>
                                            </td>
                                            <td>
                                                <%= orderData[i].userEmail %>
                                            </td>
                                            <td>₹<%= orderData[i].total %>.00</td>
                                            <td>
                                                <% if(orderData[i].status==="Ordered") { %>
                                                    <span class="orderStatus">Ordered</span>
                                                <% } else if (orderData[i].status==="Cancelled") { %>
                                                    <span class="orderStatus">Cancelled</span>
                                                <% } else if (orderData[i].status==="Shipped") { %>
                                                        <span class="orderStatus">Shipped</span>
                                                <% } else if (orderData[i].status==="Delivered" ) { %>
                                                    <span class="orderStatus">Delivered</span>
                                                <% } %>
                                            </td>
                                            <td>
                                                <%= orderData[i].date %>
                                            </td>
                                            <td>
                                                <%= orderData[i].time %>
                                            </td>
                                            <td>
                                                <a href="/admin/orderInfo?id=<%= orderData[i]._id %>"><button style="background-color: rgb(255, 255, 255); border: none;">
                                                    <svg xmlns="http://www.w3.org/2000/svg" height="12" width="10.5" viewBox="0 0 448 512"><path d="M0 96C0 78.3 14.3 64 32 64H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 128 0 113.7 0 96zM0 256c0-17.7 14.3-32 32-32H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32c-17.7 0-32-14.3-32-32zM448 416c0 17.7-14.3 32-32 32H32c-17.7 0-32-14.3-32-32s14.3-32 32-32H416c17.7 0 32 14.3 32 32z"/></svg>
                                                </button></a>
                                            </td>
                                        </tr>
                                    <% } %>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            
            <div class="card mb-4" id="returndetails" style="display: none;">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead class="text-center " style="background-color: rgb(223, 223, 223);">
                                <tr>
                                    <th>OrderID</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Time</th>
                                    <th scope="col"> Action </th>
                                </tr>
                            </thead>
                            
                            <tbody class="text-center">
                                <% for( let i=0; i < orderData.length; i++ ) { %>
                                    <% if (orderData[i].status==="Return order processing" || orderData[i].status==="Returned order recieved" || orderData[i].status==="Return order cancel") { %>
                                        <tr>
                                            <td>
                                                <%= orderData[i].orderID %>
                                            </td>
                                            <td>
                                                <%= orderData[i].userEmail %>
                                            </td>
                                            <td>₹<%= orderData[i].total %>.00</td>
                                            <td>
                                                <% if(orderData[i].status==="Return order cancel") { %>
                                                    <span class="orderStatus">Return cancelled</span>
                                                <% } else if (orderData[i].status==="Return order processing") { %>
                                                    <span class="orderStatus">Return processing</span>
                                                <% } else if (orderData[i].status==="Returned order recieved") { %>
                                                    <span class="orderStatus">Return recieved</span>
                                                <% } %>
                                            </td>
                                            <td>
                                                <%= orderData[i].date %>
                                            </td>
                                            <td>
                                                <%= orderData[i].time %>
                                            </td>
                                            <td>
                                                <a href="/admin/orderInfo?id=<%= orderData[i]._id %>"><button style="background-color: rgb(255, 255, 255); border: none;">
                                                    <svg xmlns="http://www.w3.org/2000/svg" height="12" width="10.5" viewBox="0 0 448 512"><path d="M0 96C0 78.3 14.3 64 32 64H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32C14.3 128 0 113.7 0 96zM0 256c0-17.7 14.3-32 32-32H416c17.7 0 32 14.3 32 32s-14.3 32-32 32H32c-17.7 0-32-14.3-32-32zM448 416c0 17.7-14.3 32-32 32H32c-17.7 0-32-14.3-32-32s14.3-32 32-32H416c17.7 0 32 14.3 32 32z"/></svg>
                                                </button></a>
                                            </td>
                                        </tr>
                                    <% } %>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </section> 
