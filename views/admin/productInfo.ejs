<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <title>FURNIX Admin</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="/admin-assets/imgs/theme/favicon-16x16.png">
    <!-- Template CSS -->
    <link href="/admin-assets/css/main.css" rel="stylesheet" type="text/css" />
</head>
<style>
     .custom-butto1 {
        width: 100px;
        height: 40px;
        padding: 5px;
        background-color: rgb(255, 255, 255);
        color: rgb(0, 0, 0);
        font-size: 14px;
        border-radius: 50px;
    }
</style>

<body>
    <header class="main-header navbar" style="margin-bottom: 70px;">
        <div class="col-search">
                
        </div>
        <div class="col-nav">
            <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"> <i
                    class="material-icons md-apps"></i> </button>
            <ul class="nav">
                <li class="dropdown nav-item">
                    <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdownAccount"
                        aria-expanded="false"> <img class="img-xs rounded-circle"
                            src="/admin-assets/imgs/people/profile.jpg" alt="User"></a>
                    <div class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownAccount">
                        <a class="dropdown-item text-danger" href="/admin/adminLogout"><i
                                class="material-icons md-exit_to_app"></i>Logout</a>
                    </div>
                </li>
            </ul>
        </div>
    </header>
        <section class="content-main col-lg-8">
            <div class="card mb-4">
                <div class="card-header" style="height:150px; background-color: black;">
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-xl col-lg flex-grow-0" style="flex-basis:230px">
                            <div class="img-thumbnail shadow w-100 bg-white position-relative text-center" style="height:190px; width:200px; margin-top:-120px">
                                <img src="/uploads/<%= info.images[0] %>" class="center-xy img-fluid" alt="Logo Brand">
                            </div>
                        </div> <!--  col.// -->
                        <div class="col-xl col-lg">
                            <h3><%= info.name %></h3>
                            <p><%= info.description %></p>
                        </div>
                    </div> <!-- card-body.// -->
                    <hr class="my-4">
                    <div class="row g-4">
                      
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Category</h6>
                            <p> <%= info.categoryName %></p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Price</h6>
                            <p>₹<%= info.regularPrice %>.00</p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Offer price</h6>
                            <p>₹<%= info.offerPrice %>.00</p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Offer percentage</h6>
                            <p><%= info.offerPercentage %>% off</p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Seating capacity</h6>
                            <p><%= info.capacity %> person</p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Upholstery material</h6>
                            <p><%= info.material %></p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Stock</h6>
                            <p><%= info.stock %></p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Color</h6>
                            <p><%= info.color %></p>
                        </div>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
                            <h6>Status</h6>

                            <% if (info.isBlocked === false) { %>
                                <p>Unblocked</p>
                            <% } else { %>
                                <p>Blocked</p>
                            <% } %>
                        </div>
                        
                    </div>
                </div>
            </div>
            <div class="justify-content-center align-items-center d-flex">
                <button class="custom-butto1" onclick="goBack()">Go back</button>
            </div>
        </section>
    <script src="/admin-assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="/admin-assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="/admin-assets/js/vendors/select2.min.js"></script>
    <script src="/admin-assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="/admin-assets/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="/admin-assets/js/main.js" type="text/javascript"></script>
</body>
<script>
    function goBack() {
        window.history.back();
    }
</script>

