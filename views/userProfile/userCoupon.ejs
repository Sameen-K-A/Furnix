<style>
   .firstbook{
        width: 280px;
        height: 280px;
        border-radius: 40px;
        margin-bottom: 20px;
        background: linear-gradient(315deg, rgba(9,6,6,1) 0%, rgb(94, 82, 79) 100%);
        position: relative;
        gap: 10px;
        padding-left: 10px;
        padding-right: 10px;
    }
    .swal2-container {
        background-color: transparent !important;
    }
    .swal2-popup {
        background-color: rgb(0, 0, 0);
        color: white !important;
    }
   .admincpn{
        width: 280px;
        height: 280px;
        border-radius: 40px;
        margin-bottom: 20px;
        background: linear-gradient(315deg, rgba(54,54,54,1) 0%, rgba(0,0,0,1) 48%, rgba(48,48,48,1) 71%, rgba(0,0,0,1) 100%);
        position: relative;
        gap: 10px;
        padding-left: 10px;
        padding-right: 10px;
    }
    .event-info {
        text-align: left !important;
    }
    .event-info p {
        text-align: center; /* Center the columns */
    }
   .copybtn{
		background-color: #CCBEA5;
		height: 100%;
		width: 40px;
		color: rgb(0, 0, 0);
	}
    .wrapper {
        display: flex;
        flex-wrap: wrap;
    }
    .container {
        width: calc(33.33% - 20px);
        margin: 10px; 
    }
    @media (max-width: 768px) {
        .container {
            width: calc(50% - 20px);
        }
    }
    @media (max-width: 576px) {
        .container {
            width: calc(100% - 20px);
        }
    }
    .outofstock {
		position: absolute;
		top: 40%;
		left: 0;
		width: 100%;
		height: 50px;
		background: linear-gradient(90deg, rgba(255, 255, 255, 0) 0%, rgba(0, 0, 0, 0.824) 18%, rgb(0, 0, 0) 53%, rgba(0, 0, 0, 0.877) 82%, rgba(255, 255, 255, 0) 100%);
		z-index: 9998;
	}
	.outofstockbackground {
		position: absolute;
		width: 100%;
		height: 107%;
		background-color: rgba(255, 255, 255, 0.533);
		z-index: 1;
	}
    .swal-custom {
        z-index: 99999;
        background: linear-gradient(94deg, rgba(255, 255, 255, 0) 0%, rgba(0, 0, 0, 0.8240546218487395) 26%, rgba(0, 0, 0, 1) 53%, rgba(0, 0, 0, 0.8492647058823529) 79%, rgba(227, 227, 227, 0) 100%);
        color: #ffffff;
        width: 100%;
    }
    .swal-custom-ok-btn {
        color: black !important;
    }
</style>
